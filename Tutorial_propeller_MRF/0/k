FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.06;

boundaryField
{
    

    inlet_top
    {
        type            fixedValue;
       value           $internalField;
    }

    outlet_bottom
    {
      type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    outside_wall
    {
        type            zeroGradient;
        
    }

    rotor
    {
        type            kqRWallFunction;
       value           $internalField;
    }

    AMI1
    {
        type            cyclicAMI;;
    }

    AMI2
    {
        type            cyclicAMI;;
    }
}


// ************************************************************************* //
