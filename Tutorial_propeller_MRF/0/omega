FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.028;

boundaryField
{
   

    inlet_top
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet_bottom
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    outside_wall
    {
        type            zeroGradient;
       
    }
    
    rotor
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    AMI1
    {
        type            cyclicAMI;;
    }

    AMI2
    {
        type            cyclicAMI;;
    }
}

// ************************************************************************* //
