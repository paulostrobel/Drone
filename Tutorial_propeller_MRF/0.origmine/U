FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    

    inlet_top
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    outlet_bottom
    {
       type            zeroGradient;
    }

    outside_wall
    {
        type            zeroGradient;
       
    }

    rotor
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }

    AMI1
    {
        type            cyclicAMI;;
    }

    AMI2
    {
        type            cyclicAMI;;
    }
}


